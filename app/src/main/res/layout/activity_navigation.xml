<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Navigation"
    tools:layout_editor_absoluteY="81dp">

    <Button
        android:id="@+id/MineIndlejninger"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:layout_marginLeft="31dp"
        android:text="Mine indlejninger"
        app:layout_constraintBottom_toTopOf="@+id/Indlej"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/Udlej"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="60dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Indlej" />

    <Button
        android:id="@+id/Indlej"
        android:layout_width="160dp"
        android:layout_height="44dp"
        android:layout_marginStart="31dp"
        android:layout_marginLeft="31dp"
        android:layout_marginBottom="60dp"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/MineUdlejninger"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:text="Mine Udlejninger"
        app:layout_constraintBottom_toTopOf="@+id/Udlej"
        app:layout_constraintStart_toEndOf="@+id/MineIndlejninger" />

</android.support.constraint.ConstraintLayout>